<?xml version="1.0" encoding="UTF-8" ?>
<ehcache>
  <diskStore path="java.io.tmpdir/ehcache/center-job"/>
  <defaultCache
    maxElementsInMemory="10000"
    eternal="false"
    timeToIdleSeconds="120"
    timeToLiveSeconds="120"
    overflowToDisk="true"
    diskPersistent="false"/>

  <!--小任务缓存-->
  <cache name="jobCache"
    maxElementsInMemory="10000"
    eternal="false"
    timeToIdleSeconds="120"
    timeToLiveSeconds="120"
    overflowToDisk="true"
    diskPersistent="false">

  </cache>


</ehcache>
