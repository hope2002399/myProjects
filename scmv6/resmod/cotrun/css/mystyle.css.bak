@charset "utf-8";
/* CSS Document */
.w1000{
	position: relative;
	width: 1000px;
	margin: 0 auto;
}
.header{
	top: 15px;
	z-index: 10;
	height: 137px;
	background: url(../img/bg_header.png) no-repeat center top;
}
.header .logo{
	padding-top: 10px;
	width: 300px;
	/*border: 1px solid black;*/
}
.h-search{
	width: 196px;
	height: 30px;
	padding-left: 15px;
	margin: 33px 48px 0 25px;
	display: inline;
	background: url(../img/bg_btn01.png) no-repeat left center;
	/*border: 1px solid red;*/
}
.h-search input{
	float: left;
	width: 158px;
	height: 30px;
	border: none;
	background: url(../img/bg_btn02.jpg) repeat-x;
	line-height: 30px;
	color: #bcbcbc;
}
.h-search input.submit{
	width: 38px;
	background: url(../img/bg_btn03.png) no-repeat;
}

.weatherLink{
    width: 431px;
	margin-top: -33px;
	
}

.h-weather{
    width: 210px;
	/*margin-top: 33px;*/
	font-size: 14px;
	line-height: 30px;
	color: #9c9d9d;	
	text-align: center;
	/*border: 1px solid blue;*/
}
.h-weather a{
	color: #9c9d9d;
	transition: color 500ms ease-in 0ms;
	padding: 0 4px;
}
.h-weather a:hover{
	color: #0d4aa0;
}


.h-link{
    width: 210px;
	/*margin-top: 33px;*/
	font-size: 14px;
	line-height: 30px;
	color: #9c9d9d;	
	text-align: center;
	/*border: 1px solid yellow;*/
}
.h-link .numberStyle{
	font-size: 16px;
	color: #FF0000;	
	font-weight: 700;
	font-family:Arial,sans-serif;
}
.h-link a{
	color: #9c9d9d;
	transition: color 500ms ease-in 0ms;
	padding: 0 4px;
}
.h-link a:hover{
	color: #0d4aa0;
}

.header a.fr{
	display: inline-block;
	padding-right: 28px;
	margin-top: 33px;
	background: url(../img/bg_sign02.png) no-repeat right center;
	font-size: 14px;
	line-height: 30px;
	color: #9c9d9d;
	transition: color 500ms ease-in 0ms;
}
.header a.fr:hover{
	color: #0d4aa0;
}


 /*导航下拉 开始*/
.nav{
	width: 100%;
	position: absolute;
	left: 0;
	bottom: 0;
	height: 37px;
	border-top: 2px solid #074aa9;
}
.nav ul{
	height: 37px;
}
.nav li{
	position: relative;
	width: 124px;
	height: 37px;
	background: url(../img/bg_li01.png) no-repeat;
	margin-right: 1px;
	display: inline;
}
.nav li.last{
	width: 124px;
	margin-right: 0;
	background: url(../img/bg_li02.png) no-repeat;
}
.nav a.tit{
	display: inline-block;
	width: 100%;
	font-size: 16px;
	line-height: 37px;
	color: #fff;
	text-align: center;
}
.nav li.hover a.tit, .nav li.on a.tit{
	color: #0d4aa0;
	background: #fff;
}
.subnav{
	position: absolute;
	left: 0;
	top: 37px;
	width: 100%;
	padding-bottom: 3px;
	background: url(../img/bg_bg05.png) no-repeat bottom;
	display: none;
}
.subnav a{
	display: inline-block;
	width: 100%;
	border-top: 1px solid #f2f2f2;
	background: url(../img/bg_li03.png);
	font-size: 14px;
	line-height: 34px;
	color: #666;
	text-align: center;
}
.subnav a:hover{
	color: #0d4aa0;
	background: url(../img/bg_li04.png);
}
.sunnav-pro{
	position: absolute;
	left: -875px;
	top: 37px;
	width: 1000px;
	height: 202px;
	/* height: 334px; */
	padding: 41px 2px 35px;
	background: url(../img/bg_bg06.png) no-repeat;
	display: none;
}
.sunnav-pro .box{
	float: left;
	/*border: solid 1px red;*/
	width: 49%;
	padding-left: 0%;
	text-align: center;
	/*width: 249px;
	height: 350px;
	text-align: center;
	 */
	display:block; margin:0 auto;
	border-right: 1px solid #cccccc;
}
.sunnav-pro .box img{
    padding-left:5%;
	margin:10px 20px;
	text-align: center;
}
.sunnav-pro .box p{
    margin-left:45px;
	font-size:18px;
	font-weight:500;
	color:#003399;
	text-align: left;
}
.sunnav-pro .box.last{
	border-right: none;
}
.sunnav-pro .list{
	width: 98px;
/*	margin:0 auto;
	text-align: left;
	position: relative;
	left: -20px;
	top: -30px;
	 */
	 float: left;
}
.sunnav-pro .list a{
	position: relative;
	display: inline-block;
	width: 98px;
	border-bottom: 1px solid #ececec;
	font-size: 13px;
	line-height: 27px;
	color: #666;
	transition: color 500ms ease-in 0ms;
}
.sunnav-pro .list a:hover{
	color: #104da1;
}
.sunnav-pro .last .list a img{
	 left: 82px;
}
.sunnav-pro .list a img{
	position: absolute;
	left: 58px;
	top: -2px;
}
/*导航下拉 结束*/


/*天气下拉 开始*/
.weatherNav{
	width: 100%;
	position: relative;
	left: 0;
	bottom: 0;
	height: 30px;
	/*border-top: 2px solid #074aa9;*/

}
.weatherNav ul{
	height: 30px;

}
.weatherNav li{
	position: relative;
	width: 100%;
	height: 30px;
	padding: 0px 3px 0px 5px;
	background: url(../img/weatherNavIcon.png) no-repeat right;
	display: inline;
}
.weatherNav a.tit{
	display: inline-block;
	/*width: 100%;*/
	/*font-size: 16px;*/
	line-height: 37px;
	color: #9c9d9d;
	text-align: center;
}
.weatherNav li.hover a.tit, .weatherNav li.on a.tit{
	width: 100%;
	color: #FFFFFF;
	background: #6699ff;
}
.subWeatherNav{
    z-index:10;
	position: absolute;
	left: 5px;
	top: 35px;
	width: 100%;
	/*background: url(../img/bg_bg05.png) no-repeat bottom;*/
	display: none;
	background-color: #6699ff;
}
.subWeatherNav a{
	display: inline-block;
	border-top: 1px solid #f2f2f2;
	/*background: url(../img/bg_li03.png);*/
	font-size: 14px;
	line-height: 34px;
	color: #FFFFFF;
	text-align: center;
}
.subWeatherNav a:hover{
	color: #FFFFFF;
	/*background: url(../img/bg_li04.png);*/
}

/*天气下拉 结束*/










