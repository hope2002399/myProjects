/*首页上半部分-院务通知 -begin*/
.indexMessage{
	position: relative;
	width: 1000px;
	margin: 0 auto;
	/*border: 1px solid blue;*/
}
/*院务通知-左边*/
.bl-01, .br-02{ position: relative; overflow: hidden;height: 380px;padding: 5px 5px 5px 5px;}
.bl-01{ width: 680px; padding-right: 7px; border-right: solid 1px #e6e6e6; float: left; }
.bl-01 .sp-focus{ position: relative; }
.bl-01 .sp-focus .s-item img{ width: 680px; height: 380px; }
.bl-01 .sp-focus .bg-focus, .bl-01 .sp-focus .txt-focus{ width: 654px; height: 41px; position: absolute; top: 313px; padding: 13px; left: 0; }
.bl-01 .sp-focus .txt-focus h2{ font-size: 20px; line-height: 20px; font-weight:normal;}
.bl-01 .sp-focus .txt-focus p{ font-size: 14px; line-height: 30px; }
.bl-01 .sp-focus .txt-focus p a, .bl-01 .sp-focus .txt-focus h2 a{ color: #fff; }
.bl-01 .sp-focus .bg-focus{ background-color: #000; opacity: 0.70; filter: alpha(opacity=70)\9; }
.bl-01 #fo-prev, .bl-01 #fo-next{ cursor: pointer; position: absolute; top: 133px; width: 75px; height: 75px; opacity: 0.40; filter: alpha(opacity=40)\9; }
.bl-01 #fo-prev{ left: 5px; background: url(http://i2.sinaimg.cn/jc/main/sky2014/01/all.png) no-repeat -168px 0; }
.bl-01 #fo-next{ right: 0; padding-right: 7px; background: url(http://i2.sinaimg.cn/jc/main/sky2014/01/all.png) no-repeat -244px 0; }
.bl-01 #fo-prev:hover, .bl-01 #fo-next:hover{ opacity: 0.7; filter: alpha(opacity=70)\9; }

/*院务通知-右边*/
.br-02{ width: 295px; padding: 4px 5px 5px 0px;float: right;}
.br-02 ul{}
.br-02 ul li{position:relative; width: 285px;   padding: 3px 5px 3px 5px;height:56px; border-bottom: dashed 1px #e3e3e3;  background: url('../img/icon_01.png') no-repeat;background-position:0% 25%;}
.br-02 ul li a{ display: block;width: 100%; height:52px; padding: 0px 5px 3px 5px;color: #666666; font-size:16px; font-family:microsoft YaHei;}
.br-02 ul li a:visited{ background-color: #f9f9f9; }
.br-02 ul li a:hover{ background-color: #f9f9f9;  color: #000000;}
.br-02 ul li a:active{ outline: none; }
.br-02 span{ width:80px; height:22px;position:absolute; right:0px; bottom:4px;}
.br-02 span.time{
  font-size: 12px;
  font-family:'Arial,sans-serif'
  font-weight: bold;
  
}
.br-02 span.badge{
  width:25px; 
  min-width: 10px;
  padding: 0px 5px;
  font-size: 12px;
  font-family:'Arial,sans-serif'
  font-weight: bold;
  color: #fff;
  text-align:center;  
  background-color: #5eb95e;
}

/*院务通知-左边广告图*/
.s-item{ float: left; display: inline; position: relative; }
.spp{ text-align: center; margin-left: 5px; }
.spp span{ display: inline-block; *display: inline; *zoom: 1; width: 15px; height: 5px; margin: 10px 5px 0px 0; overflow: hidden; }
.spp .dotItemOn{ background-color: #1f90de; }
.spp .dotItem{ background-color: #434343; }
.spp-focus{ text-align: center; position: absolute; top: 324px; right: 6px; }
.spp-focus span{ display: inline-block; *display: inline; *zoom: 1; width: 15px; height: 5px; margin: 0 5px; *overflow: hidden; }
.spp-focus .dotItemOn{ background-color: #1f90de; }
.spp-focus .dotItem{ background-color: #989898; }
/*首页上半部分-院务通知-end*/







