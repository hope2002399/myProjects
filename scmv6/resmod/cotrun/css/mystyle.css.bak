/*图片新闻 begin*/

.picbox{ width:1000px;margin:0px auto; margin-top:30px}
.picbox h2{font-size:22px;line-height:36px;color:#d13a3e;text-align:left;}
/*.picbox .text{font-size:16px;line-height:30px;color:#2a2d39;padding:10px 0 20px 0;text-align:center;}*/
.slide{margin-left:1px;}
.slide:after{height:0;display:block;clear:both;visibility:hidden;content:'.';font-size:0}
/*.slide ul{ width:1000px;margin:0 auto;position:relative;}*/
.slide li{ float:left;text-align:center;cursor:pointer;z-index:0;position:relative;margin:0 10px 10px 0;width:240px;height:200px;margin-bottom:10px;box-shadow:0px 1px 0px #edecec;border: 1px solid #e5e5e5;}
.slide li.r{ margin-right:0}
.slide li .in img{*width:240px;*height:auto;}
.slide li .in{ position: absolute;left: 0;top: 0;z-index:2;width: 240px;}
.slide li .in .imgdiv{display:block;width:230px;height: 142px;margin:0 auto;margin-top:4px;overflow:hidden;}
.slide li span{ display:block;text-align: center;height:46px;padding:8px 3px 0 3px;font-size:16px;line-height:20px;color:#333333;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
/*.slide li span b{display:block;color:#d13a3e}*/
.slide li.pic .original{display:none;padding:5px;padding-bottom:0;border: 1px solid #e5e5e5;position:absolute;text-align: center;left:-16px; top:-59px; background: #fff;box-shadow:1px 1px 3px #a5a5a5;}
.slide li.pic .original .imga{display:block;}
.slide li .original img{*height:250px;*width:auto;}
.slide li .original span {height:36px;padding-top:10}
.slide li .original b{ line-height:36px;}
.slide li.on{z-index:999}
.slide li.on .original{ display:block;z-index:999;}

.slide li .in .pic_msg{display:none;}
.slide li .original .pic_msg{position:relative;display:block;width:100%;margin-top:-5px;padding-top:5px;padding-left:0px;height:25px;background:#f3f3f3;line-height:18px;vertical-align: middle;}



.slide li .original .pic_msg .time{
  float:left;
  position:relative;
  font-size: 12px;
  height:15px;
  margin-top:-8px;
  font-family:'Arial,sans-serif'; 
  /*border: 1px solid red;*/
}
.slide li .original .pic_msg .badge{
  float:right;
  width:25px; 
  height:22px; 
  margin:-1px 5px 0px 0px;
  min-width: 10px;
  padding: 0px 5px;
  font-size: 12px;
  font-family:'Arial,sans-serif'
  font-weight: bold;
  color: #fff;
  text-align:center;  
  background-color: #5eb95e;
    
  /*border: 1px solid blue;*/
}


